{
    "lyrics": [
        "Nāgendrahārāya Trilōcanāya,",
        "Bhasmāṅgarāgāya Mahēśvarāya.",
        "Nityāya Śuddhāya Digambarāya,",
        "Tasmai Na Kāraaya Namaḥ Śivāya ॥1॥",
        "Mandākinī Salilacandana Carcitāya,",
        "Nandīśvara Pramathanātha Mahēśvarāya.",
        "Mandārapuṣpa Bahupuṣpa Supūjitāya,",
        "Tasmai Ma Kāraaya Namaḥ Śivāya ॥2॥",
        "Śivāya Gaurī-vadanābjavṛnda,",
        "Sūryāya Dakṣādhvaranāśakāya.",
        "Śrīnīlakaṇṭhāya Vṛṣadhvajāya,",
        "Tasmai Śi Kāraaya Namaḥ Śivāya ॥3॥",
        "Vasiṣṭhakumbhōdbhavagautamārya,",
        "Munīndrādēvārchitaśēkharāya.",
        "Candrārka Vaiśvānaralōcanāya,",
        "Tasmai Va Kāraaya Namaḥ Śivāya ॥4॥",
        "Yakṣasvarūpāya Jaṭādharāya,",
        "Pinākahastāya Sanātanāya.",
        "Divyāya Dēvāya Digambarāya,",
        "Tasmai Ya Kāraaya Namaḥ Śivāya ॥5॥",
        "Pañcākṣaramidaṁ Puṇyaṁ Yaḥ Paṭhēcchivasannidhau.",
        "Śivalōkamavāpnōti Śivēna Saha Mōdatē."
    ]
}