{
  "lyrics": [
    "ماهِمْنَهْ بارَمْ تِهْ بَرَمَفِدوُشو يَدْيَسَدْرِشي",
    "سْتُتِ بْرَهْمادِينامَفِ تَدَفَسَنّاسْتْوَي جِرَحْ",
    "أَثَ أواچْيَهْ سَرْفَهْ سْوَمَتِبَرِنامافَذِي غْرِنَنْ",
    "مَمابْيِشَ سْتُوتْرِ هَرَ نِرَفَادَحْ بَرِكَرَحْ",
    "أَتِيتَهْ بَنْثانَمْ تَفَ چَ مَهِمَا وانْمَنَسَيُهْ",
    "أَتَدْفْيافْرِتْيَا يَمْ چَكِتَمَبِهَدَتِّ شْرُتِرَفِ",
    "سَ كَسْيَ سْتُوتَفْيَحْ كَتِفِذَغُنَحْ كَسْيَ فِشَيَحْ",
    "بَدِ تْوَرْفاتْشِينِ بَتَتِ نَ مَنَحْ كَسْيَ نَ فاچَحْ",
    "مَدْهُسْفِيتَا فاچَهْ بَرَمَمَمْرِتَمْ نِرْمِتَفَتَحْ",
    "تَفَ بَرَهْمَنْ كِمْ فاجَفِ سُرَغُرُورْفِسْمَيَفَدَمْ",
    "مَمَ تْفِيتامْ فانِيمْ غُنَكَثَنَفُنْيَنَ بَهَفَتَحْ",
    "بُنامِيتْيَرْثِأسْمِنْ بُرَمَثَنَ بُدْدِرْفَيَفَسِتا",
    "تَفَيْشْوَرْيَمْ يَتَّجَّغَدُدَيَرَكْشابَرَلَيَكْرِتْ",
    "تَرَيِيفَسْتُ فِيَسْتَمْ تِسْرُشُ غُنَبِهِنّاسُ تَنُشُ",
    "أَبَهَفْيَانامَسْمِنْ فَرَدَ رَمَنِيامَرَمَنِيمْ",
    "فِهَنْتُمْ فْياكْرُوشِيمْ فِدَدَتَ إِهَيْكِ جَدَدِهِيَحْ",
    "كِمِيهَحْ كِمْكاياحْ سَ خَلُ كِمُبايَسْتْرِبْهُوَانَمْ",
    "كِمَاذارُو ذاتا سْرِجَتِ كِمُباداتَ إِتِ چَ",
    "أَتَرْكَيَشْوَرْيِ تْفَيَنْيَفَسَرَ dُهْسْتُو هَتَدِهِيَحْ",
    "كُتَرْكُوَيَمْ كامْشِتْ مُخَرَيَتِ مُهَايَ جَغَتَحْ",
    "أَجَنْمَانُو لُوكاحْ كِمَفَيَفَفَنْتُوپِي جَغَتامْ",
    "أَدِهْشْتاتارَمْ كِمْ بَهَفَذِهِيَرَنادْرِتْيَ بَهَفَتِ",
    "أَنِيشُو وا كُرْيادْ بُهُوَانَجَنَنِ كَحْ بَرِكَرُو",
    "يَتُو مَنْداسْتْوامْ بَرَتْيَمَرَفَرَ سَمْشِرَتَ إِمِ",
    "تَرَيِ سانْخْيَمْ يوغَحْ بَشُوبَتِمَتَمْ فَيْشْنَفَمِتِ",
    "بَرَبْهِنِّ بَرَسْثانِ بَرَمِدَمَدَحْ بَثْيَمِتِ چَ",
    "رُوشِينامْ فَيْشِتْرِيادْرُجُكُتِلَ نانابَثَجُشامْ",
    "نْرِنامِكُو غَمْيَسْتْفَمَسِ بَيَسامَرْنَفَ إِفَ",
    "مَهُوكْشَحْ خَتْوانْغَمْ بَرَشُورَجِنَمْ بَهَسْمَ فَنِنَحْ",
    "كَبالمْ چِيتِيَتَّفَ فَرَدَ تَنْتْروبَكَرَنَمْ",
    "سُرَاسْتامْ تامْرِدْدِهِمْ دَدَهَتِ تُ بَهَفَدْبُوبَرَنِهِتامْ",
    "نَ هِ سْواتْمارامَمْ فِشَيَمْرِغَتْرِشْنا بَرَمَيَتِ",
    "دْرُوامْ كَشْتِ سَرْفَ سَكَلَمَفَرَسْتْوَذْرُوَمِدَمْ",
    "بَرُو دْرَوْفْياذْرَوْفْيِ جَغَتِ غَدَتِ فِيَسْتَفِشَيِ",
    "سَمَسْتِأَبْيَتَسْمِنْ بُرَمَثَنَ تَيْرْفِسْمِتَ إِفَ",
    "سْتُوفَنْ جِهْرِمِ تْوامْ نَ خَلُ نَنُ دْرِشْتا مُخَرَتَا",
    "تَفَيْشْوَرْيَمْ يَتْنَادْ يَدُبَرِ فِرِنْشِرْهَرِرَدَهَ",
    "بَرِشْشِتُمْ ياتافَنَلَمَنَلَسْكَنْذَفَبُشَحْ",
    "تَتُو بَهَكْتِشْرَدْهابَرَغُرُغْرِنَدْبْيامْ غِرِشَ يَتْ",
    "سَفَيَمْ تَسْتِ تابْيامْ تَفَ كِمَنُفْرِتِرْنَ فَلَتِ",
    "أَيَتْنَاداسادْيَ تْرِبْهُوَانَمَفَيْرَفْيَتِكَرَمْ",
    "دَشاسْيُو يَدْباهُونَبْرِتَ رَنَكَنْدُو-بَرَفَشَانْ",
    "شِرَحْبَدْمَشْرِينِ-رَشِتَشَرَنَامْبُورُهَ-بَلِحْ",
    "سْتِراياسْتْفَدْبَهَكْتِسْتْرِبُرَهَرَ فِسْفُورْجِتَمِدَمْ",
    "أَمُشْيَ تْفَتْسِفا-سَمَدْهِغَتَسارَمْ بُجَفَنَمْ",
    "بَلاتْ كَيْلَاسِأَبِي تْفَدَدْهِفَسَتَوْ فِكْرَمَيَتَحْ",
    "أَلَبْهِيَ بَاتالِأَبِي-أَلَسَشَلِتانْغُشْتَشِرَسِ",
    "بَرَتِشْتَ تْفَياسِيدْ دْرُوَامُبَشِتُو مُهْيَتِ خَلَحْ",
    "يَدْرِدْدِهِمْ سُتْرَامْنُو فَرَدَ بَرَمُوتْشَيْرَفِ سَتِيمْ",
    "أَدَهَشْشَكْرِ بَانَحْ بَرِجَنَفِذِهِيَتْرِبْهُوَانَحْ",
    "نَ تَشِتْرَمْ تَسْمِنْ فَرِفَسِتَرِ تْفَتْشَرَنَيُوحْ",
    "نَ كَسْيَابْيُنَتْيَ بَهَفَتِ شِرَسَسْتْفَيَفَنَتِحْ",
    "أَكَنْدَ-بَرَهْمَانْدَ-كْشَيَشَكِتَ-دِفاسُرَكْرِبَا",
    "فِذِهِيَسْيَأَسِيدْ يَسْتْرِنَيَنَ فِشَمْ سَمْهْرِتَفَتَحْ",
    "سَ كَلْمَاشَحْ كَنْتِه تَفَ نَ كُرُوتِ نَ شْرِيَمَهُو",
    "فِكَرُوَفِ شْلَاغْيُو بُهُوَانَ-بَهَيَ-بَهَنْغَ-فَيَسَنِنَحْ",
    "أَسِدْهَارْثَا نَيْفَ كْوَشِدَفِ سَدِفاسُرَنَرِ",
    "نِفَرْتَنْتِ نِتْيَمْ جَغَتِ جَيِنُو يَسْيَ فِشِخَاحْ",
    "سَ بَشْيَنِيشَ تْفَامِتَرَسُرَسَاذَارَنَمَبُوتْ",
    "سَمَرَحْ سْمَرْتَفْيَاتْمَ نَ هِ فَشِشُ بَثْيَحْ بَرِبَهَفَحْ",
    "مَهِي بَادَاغَاتادْ فَرَجَتِ سَهَسَا سَمْشَيَفَدَمْ",
    "بَدَمْ فِشْنُورْبْرامْيَدْ بُهُجَ-بَرِغَ-رُغْنَ-غَرَهَ غَنَمْ",
    "مُهُرْدَيُورْدَوْسْتْيَمْ يَاتْيَنِبْرِتَ-جَتا-تادِتَ-تَتَا",
    "جَغَدْرَكْشَايَ تْفَمْ نَتَسِ نَنُ فامَيْفَ فِبُوتَا",
    "فِيَدْفَيَابِي تَارَا-غَنَ-غُنِتَ-فِينُوذْغَمَ-رُتِشْ",
    "بَرَفاهُو فارامْ يَحْ بْرِشَتَلَغُدْرِشْتَحْ شِرَسِ تِ",
    "جَغَدْدْفِيباكَرَمْ جَلَدِهِيَفَلَيَمْ تِنَ كْرِتَمِتِ",
    "أَنِنَفُونَيَمْ دْرِتَمَهِمَ دِفْيَمْ تَفَ فَبُحْ",
    "رَتَحْ كْشُونِ يَنْتَا شَتَدْرِتِرَغِنْدْرُو دَنُورَتَحْ",
    "رَتَانْغِ شَنْدْرارْكَوْ رَتَحْ-چَرَنَ-بَانِحْ شَرَ إِتِ",
    "دِذَكْشُسْتِ كُوَيَمْ تْرِبُرَتْرِنَمادَمْبَرَفِذِحْ",
    "فِذِهِيَحْ كْرِيدَنْتْيُو نَ خَلُ بَرَتَنْتْرَاحْ بَرَبُذِيَحْ",
    "هَرِسْتِ ساهَسْرَمْ كَمَلَ بَلِمَاذَايَ بَدَيُوحْ",
    "يَدِكُونِ تَسْمِنْ نِجَمُدَهَرَنَتْرَكَمَلَمْ",
    "غَتُو بَهَكْتِيُدْرِكَحْ بَرِنَتِمَسَوْ شَكْرَفَبُشَحْ",
    "تَرَيَانامْ رَكْشايَ تْرِبُرَهَرَ جاغَرْتِ جَغَتامْ",
    "كَرَتَوْ سُبْتِ جاغَرَتْ تْفَمَسِ فَلَيُوغِ كَرَتُمَتامْ",
    "كْوَ كَرْمَ بَرَذْفَسْتَمْ فَلَتِ بُرُشَارَاذَنَمْرِتِ",
    "أَتَسْتْفامْ سَمْبْرِكْشَ كَرَتُشُ فَلَدَانَ-بَرَتِبُهُفَمْ",
    "شْرُتَوْ شْرَدْهامْ بَدْفَا دْرِذَفَرِكَرَحْ كَرْمَسُ جَنَحْ",
    "كْرِيادَكْشُ دَكْشَحْ كَرَتُوبَتِرَدْهِيشَسْتَنُبْرِتامْ",
    "رِشِينا مارْتْفِجْيَمْ شَرَنَدَ سَدَسْيَاحْ-سُرَغَنَاحْ",
    "كَرَتُوبْرَنْشَسْتْفَتَحْ كَرَتُوفَلَفِذَانَفَيَسَنِنَحْ",
    "دْرُوامْ كَرْتُحْ شَرَدْهَافِذُرَمَبِهْشَرَايَ هِ مَخَاحْ",
    "بَرَجَانَاتْهَمْ ناتَحْ بَرَسَبَهَمَبِكَمْ سْوَامْ دُهُتَرَمْ",
    "غَتَمْ رُوهِدْ بُوتامْ رِرَمَيِشُمَرِشْيَسْيَ فَبُشَا",
    "دَنُوشْبَانِرْيَاتَمْ دِفَمَبِ سَفَتْرَاكْرِتَمَمُمْ",
    "تَرَسَنْتَمْ تِأَدْيَابِ تْيَجَتِ نَ مْرِغَفْيَاذَرَبَهَسَحْ",
    "سْفَلَافَنْيَاشَمْسَا دْرِتَدَنُوشَمَهْنَايَ تْرِنَفَتْ",
    "بُرَحْ بْلُشْتَمْ دْرِشْتْفَ بُرَمَثَنَ بُشْبَايُدَمَبِ",
    "يَدِ سْتْرَيْنَمْ دِيفِي يَمَنِرَتَ-دِهَارْذَ-غَتَناتْ",
    "أَفَيْتِ تْفامَدْها بَتَ فَرَدَ مُغْدْها يُوفَتَيَحْ",
    "شْمَشانِشْواكْرِيدَا سْمَرَهَرَ بَشاتْشَاحْ سَهَشَراحْ",
    "شِتا-بَهَسْمالِبَحْ سَرَغَفِ نْرِكَرُوتِي-بَرِكَرَحْ",
    "أَمَنْغَلْيَمْ شِيلَمْ تَفَ بَهَفَتُ نامَيْفَمَكِلَمْ",
    "تَثابِ سْمَرْتْرِينا فَرَدَ بَرَمَمْ مَنْغَلَمَسِ",
    "مَنَحْ بَرَتْيَكْشِتْتِ سَفِذَمَفِذاياتَ-مَرُتَحْ",
    "بَرَهْرِشْيَدْرُومانَحْ بَرَمَدَ-سَلِلُوتْسَنْغَتِ-دْرِشَحْ",
    "يَدالُوكْيَاهْلادَمْ هْرَدَ إِفَ نِمَجْيَامْرِتَمَيِ",
    "دَدَهْتْيَنْتَسْتَتْفَمْ كِمَبِ يَمِنَسْتَتْ كِلَ بَهَفانْ",
    "تْفَمَرْكَسْتْفَمْ سُومَسْتْفَمَسِ بَفَنَسْتْفَمْ هُوتَفَهَحْ",
    "تْفَمابَسْتْفَمْ فَيُومَ تْفَمُ دْهَرَنِراتْمَ تْفَمِتِ چَ",
    "بَرِشْشِنَّامِفَمْ تْفَيِ بَرِنَتا بِبْرَتِ جِرَمْ",
    "نَ فِدْمَسْتَتْتَفَمْ فَيَمِهَ تُ يَتْ تْفَمْ نَ بَهَفَسِ",
    "تَرَيِيمْ تِسْرُو فْرِتِيسْتْرِبْهُوَانَمَثُو تْرِينَبِ سُرَانْ",
    "أَكَرَادْيَيْرْفَرْنَيْسْتْرِبِرَبِهْدَتْ تِيرْنَفِكْرِتِ",
    "تُرِيَمْ تِهْ دْهامَ دْهْوَنِبِرَفَرُنْذانَمَنُبِحْ",
    "سَمَسْتَمْ فِيَسْتَمْ تْفامْ شَرَنَدَ غْرِناتْيُومِتِ بَدَمْ",
    "بَهَفَحْ شَرْفُو رُودْرَحْ بَشُوبَتِرَثُوغْرَحْ سَهَمَهَانْ",
    "تَثا بْهِيمِشانافِتِ يَدَبِهْذاناشْتَكَمِدَمْ",
    "أَمُشْمِنْ بَرَتْيَكَمْ بَرَفِشَرَتِ دِيفَ شْرُتِرَفِي",
    "بْرِيَاياسْمَيْدْهامْنِ بْرَنِهِتَ-نَمَسْيُوَسْمِ بَهَفَتِ",
    "نَمُو نِدِشْتَايَ بْرِيَدَفَ دَفِشْتَايَ چَ نَمَحْ",
    "نَمَحْ كْشُودِشْتَايَ سْمَرَهَرَ مَهِشْتَايَ چَ نَمَحْ",
    "نَمُو فَرْشِشْتَايَ تْرِنَيَنَ يَفِشْتَايَ چَ نَمَحْ",
    "نَمَحْ سَرْفَسْمَيْ تِ تَدِدَمَتِسَرْفَايَ چَ نَمَحْ",
    "بَهُلَ-رَجَسِ فِشْفُوتْبَتَوْ بَهَفَايَ نَمُو نَمَحْ",
    "بَرَبَلَ-تَمَسِ تَتْ سَمْهَارِ هَرَايَ نَمُو نَمَحْ",
    "جَنَ-سُوخَكْرِتِ سَتْفُودْرِكْتَوْ مْرِدَايَ نَمُو نَمَحْ",
    "بَرَمَهَسِ بَدِ نِسْتْرَيْغُنْيِ شِفَايَ نَمُو نَمَحْ",
    "كْرِشَ-بَرِنَتِ-شِتَحْ كْلِشَفَشْيَمْ كْوَ چِدَمْ",
    "كْوَ چَ تَفَ غُنَ-سِيمُولَنْغِنِي شَشْفَدْرِدْهِحْ",
    "إِتِ چَكِتَمَمَنْدِيكْرِتْيَ مامْ بَهَكْتِرَاذادْ",
    "فَرَدَ چَرَنَيُوسْتِ فَاكْيَ-بُشْبُوبَهَارَمْ",
